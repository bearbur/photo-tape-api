{
  "name": "photo-tape-api",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "npm run prestart && node NODE_ENV=PRODUCTION .",
