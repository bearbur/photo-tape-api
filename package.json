    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "dependencies": {
    "express": "4.17.1",
    "express-winston": "4.0.5",
    "winston": "3.3.3",
    "mongoose": "5.11.13",
    "jsonwebtoken": "8.5.1"
  },
  "devDependencies": {
    "@types/express": "4.17.9",
    "@types/node": "14.14.10",
    "@types/mongoose": "5.10.3",
    "@types/jsonwebtoken": "8.5.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.1.2",
    "nodemon": "2.0.7"
  }
}
